
footer
	display none
.contenidos
	bottom 0 
	left 0
	position absolute
	right 0
	top 85px
	width 100%
	.contLeft
		bottom 0 
		left 0
		margin 0 420px 0 80px 
		position absolute 
		right 0
		top 0
	.contRight
		bottom 0
		float right
		margin 0
		position absolute
		right 0 
		top -5px
		width 420px
		.listaDias
			display block
			.event
				margin-left 14px
				width 94%
		.timelineOuter
			height 100%
			position fixed
			right 207px
			x-index 0
			.timelineInner
				background rgba(black,.3)
				height 100vh
				left 50%
				position absolute
				width 4px
		.tituloOuter
			display block
			height auto
			position absolute
			top -50px
			left 8px
			z-index 6
.header
	border-bottom 5px solid verde
	height 85px
	position absolute
	top 0
	right 0
	left 0
	.heroOuter
		.hero
			background gris
			animation animacionTablet 150s infinite
			-webkit-animation animacionTablet 150s infinite
			.heroInner
				margin -40px 0 0 0 
				background-position 5px 7px
				background-size 15% auto
				transform: skew(0deg);
				-ms-transform: skew(0deg);
				-webkit-transform: skew(0deg);
	.logoOuter
		h1
			line-height 25px
			font-size 20px
			margin -60px 0 0 115px
			text-align left
		.logoInner
			position relative
			figure
				position absolute
				top 10px 
				left 20px
				img
					display block
					margin 0 auto
					width 60px
.llamadaLabel
.llamadaTitulo
	display none
.menu
	background menuLateral
	bottom 0
	display block
	left 0 
	position absolute
	top 85px 
	width 80px
	ul
		color rgba(white,.8)
		text-align center
		.botonOuter
			cursor pointer
			height 80px
			.botonInner
				position relative
				text-shadow 0 2px 5px rgba(black,.6)
				width 100%
				h3 
					bottom 28px
					font-size .55em
					position relative
				.icono
					&:after
						bottom 24px
						display block
						font-family icomoon
						font-size 2em
						left 38%
						position absolute
				.contactar
					&:after
						content email
				.info
					&:after
						content info
				.patro
					color verde
					&:after
						content colaboradores
				.pubilles
					&:after
						content pubillas
			.top
				margin 85px 0 0 0 
			.bottom
				bottom 0px
				position absolute


// Acciones CHECKBOX
#llamadaCalendario
	display none
#llamadaCalendario:checked ~ .calendario 
	margin 0 
#llamadaCalendario:checked ~ .contenidos 
	margin 0
#llamadaCalendario:checked ~ .contenidos .calendario .listaDias 
	display block
#llamadaCalendario:checked ~ .contenidos .calendario .tituloOuter 
	display block


// ANIMACIONES
@keyframes animacionTablet
	0%
		transform translate(-100%, -100%)
		-ms-transform translate(-100%, -100%)
		-webkit-transform translate(-100%, -100%)
	50%
		transform translate(0%, 40%)
		-ms-transform translate(0%, 40%)
		-webkit-transform translate(0%, 40%)

	100%
		transform translate(-100%, -100%)
		-ms-transform translate(-100%, -100%)
		-webkit-transform translate(-100%, -100%)