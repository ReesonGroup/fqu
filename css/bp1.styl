.contieneInformacion
	.informacion
		background negroInterface3
		position relative
		.infoHeader
			background blancoManchado
			border-bottom 1px solid borde
			color texto2
			height 80px
			h2
				font-size 21px
				font-style italic
				letter-spacing 1px
				padding 15px 0 0 30px
				text-transform uppercase
				span
					display block
					font-size 16px 
					letter-spacing 0px
					line-height 21px
					text-transform lowercase
		.infoOuter
			.infoInner
				border-bottom 1px solid #e6e6e6
				.checkInfo
					height 70px 
					position relative
					width 100%
					z-index 20
					.puntos
						&:after
							color borde
							content puntos
							font-family icomoon
							font-style normal
							padding 5px 0 5px 20px
							position absolute
							right 20px
							top 20px
				.entrades
					margin -70px 0 0 0
					&:before
						content ticket
						font-family icomoon
						font-size 18px 
						font-style normal
						left 20px
						position absolute
						top 30px
				.inscripcions
					margin -70px 0 0 0
					&:before
						content calendario
						font-family icomoon
						font-size 18px 
						font-style normal
						left 20px
						position absolute
						top 30px
				.horario
					margin -70px 0 0 0
					&:before
						content reloj
						font-family icomoon
						font-size 18px 
						font-style normal
						left 20px
						position absolute
						top 30px
				.notes
					margin -70px 0 0 0
					&:before
						content notas
						font-family icomoon
						font-size 14px 
						font-style normal
						left 20px
						position absolute
						top 30px
				.post
					background white
					color rgba(texto2,.7)
					font-size 16px
					font-style italic
					line-height 21px
					padding 28px 0 28px 0
					position relative
					h4
						margin 0 78px 0 55px
					.detallePost
						border-left none
						h5
							color rgba(texto2,.7)
							font-style normal
							margin 20px 20px 10px 55px
							&:first-child
								margin 40px 20px 10px 55px
						p
							color rgba(black,.6)
							font-style normal
							font-weight 400
							margin 0 30px 0 55px
							&:last-child 
								margin 0 20px 30px 55px
						p.detallePostPiscina
							margin 20px 20px 30px 55px
						ul
							margin 20px 30px 0 55px
							li 
								color rgba(black,.6)
								font-style normal
								font-weight 400
								list-style-type circle
								margin 0 0 10px 0
								&:last-child 
									margin 0 0 30px 0
					&:last-child
						border-bottom none
