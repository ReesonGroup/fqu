
.botonera
	background blancoManchado
	border 1px solid borde
	border-left 5px solid azul
	border-right 5px solid azul
	height auto
	position relative
	width 100%
	z-index 6
	&:after
		border-bottom 37px solid transparent
		border-left 10px solid transparent
		border-right 8px solid azul
		border-top 37px solid transparent
		content ''
		position absolute
		right 0px
		top 0
	ul
		overflow-x auto
		padding 12px 0
		white-space nowrap
		&::-webkit-scrollbar
			display none
		li
			background azul
			border-radius 50%
			display inline-block
			height 50px
			margin 0 20px 0 0 
			padding 5px
			position relative
			text-align center
			width 50px
			&:first-child
				margin 0 20px 0 20px
			a
				color white
				font-style italic
				text-decoration none
				span 
					font-size .5em
					left 30%
					position absolute
					top 28px
		.botoneraActive
			background #1d5c90
			// box-shadow 0 2px 4px rgba(black,.4) inset
			a
				// text-shadow 0 2px 4px rgba(black,.4)
.contieneColaboradores
	.colaboradores
		background white
		color rgba(black,.7)
		h2
			font-size 24px
			font-weight 300
			padding 30px 20px 20px 20px
			text-align center
		.logoOuter
			padding 0 20px 100px 20px 
			text-align center
			.logoInner
				display inline-block
				padding 15px
				vertical-align top
				width 60%
				img
					width 100%
.contieneInformacion
	.informacion
		background negroInterface3
		position relative
		.infoHeader
			background blancoManchado
			border-bottom 1px solid borde
			color texto2
			height 80px
			h2
				font-size 21px
				font-style italic
				letter-spacing 1px
				padding 15px 0 0 30px
				text-transform uppercase
				span
					display block
					font-size 16px 
					letter-spacing 0px
					line-height 21px
					text-transform lowercase
		.infoOuter
			.infoInner
				border-bottom 1px solid #e6e6e6
				.checkInfo
					height 70px 
					position relative
					width 100%
					z-index 20
					.puntos
						&:after
							color borde
							content puntos
							font-family icomoon
							font-style normal
							padding 5px 0 5px 20px
							position absolute
							right 20px
							top 20px
				.entrades
					margin -70px 0 0 0
					&:before
						content ticket
						font-family icomoon
						font-size 18px 
						font-style normal
						left 20px
						position absolute
						top 30px
				.inscripcions
					margin -70px 0 0 0
					&:before
						content calendario
						font-family icomoon
						font-size 18px 
						font-style normal
						left 20px
						position absolute
						top 30px
				.horario
					margin -70px 0 0 0
					&:before
						content reloj
						font-family icomoon
						font-size 18px 
						font-style normal
						left 20px
						position absolute
						top 30px
				.notes
					margin -70px 0 0 0
					&:before
						content notas
						font-family icomoon
						font-size 14px 
						font-style normal
						left 20px
						position absolute
						top 30px
				.post
					background white
					color rgba(texto2,.7)
					font-size 16px
					font-style italic
					line-height 21px
					padding 28px 0 28px 0
					position relative
					h4
						margin 0 78px 0 55px
					.detallePost
						border-left none
						h5
							color rgba(texto2,.7)
							font-style normal
							margin 20px 20px 10px 55px
							&:first-child
								margin 40px 20px 10px 55px
						p
							color rgba(black,.6)
							font-style normal
							font-weight 400
							margin 0 30px 0 55px
							&:last-child 
								margin 0 20px 30px 55px
						p.detallePostPiscina
							margin 20px 20px 30px 55px
						ul
							margin 20px 30px 0 55px
							li 
								color rgba(black,.6)
								font-style normal
								font-weight 400
								list-style-type circle
								margin 0 0 10px 0
								&:last-child 
									margin 0 0 30px 0
					&:last-child
						border-bottom none
.tituloAra
	-webkit-animation timeLineLento .8s
	animation timeLineLento .8s 
	height auto
	left 0
	position absolute
	top 430px
	width 100%
	z-index 10
	.tituloInner
		background verdeOscuro
		border-radius 5px 5px 0 0 
		box-shadow 0 -2px 16px rgba(black,.7)
		height 72px
		left 50%
		margin -2px 0 0 -73px
		position relative 
		width 145px
		&:after
			border-bottom 37px solid transparent
			border-left 70px solid transparent
			border-right 70px solid transparent
			border-top 15px solid verdeOscuro
			bottom -52px
			content ""
			content ''
			left 0
			position absolute
			right 0px
		p
			color rgba(white,1)
			font-family Oswald
			font-size 1.2em
			font-weight 400
			left 14px
			letter-spacing 1px
			line-height 31px
			margin 0 
			position absolute
			text-transform uppercase
			top 9px
			transform: scale(1, .9);
			-ms-transform: scale(1, .9);
			-webkit-transform: scale(1, .9);
			span
				display block
				font-weight 700
				letter-spacing .5px
				transform: scale(1, 1.3);
				-ms-transform: scale(1, 1.3);
				-webkit-transform: scale(1, 1.3);
