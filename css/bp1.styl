.wraper
	overflow hidden

// Acciones CHECKBOX
#llamadaCalendario
	display none
#llamadaCalendario:checked ~ footer
	width 100%
#llamadaCalendario:checked ~ .calendario 
	margin -80px 0 0 0 
#llamadaCalendario:checked ~ .contenidos 
	// Esto hace que al salir del canvas no tenga altura
	margin-left -100%
#llamadaCalendario:checked ~ .contenidos .calendario .listaDias 
	display block
#llamadaCalendario:checked ~ .contenidos .calendario .timelineOuterbp1 
	display block
#llamadaCalendario:checked ~ .contenidos .calendario .tituloOuter 
	display block

#llamadaCalendario:checked ~ .llamadaLabel
	background none
#llamadaCalendario:checked ~ .llamadaLabel .boton 
	transform rotateZ(180deg)
	-ms-transform rotateZ(180deg)
	-webkit-transform rotateZ(180deg)
#llamadaCalendario:checked ~ .llamadaTitulo
	margin-left -100%


// ANIMACIONES
@keyframes timeLine
	0%
		opacity 0
	100%
		opacity 1

@keyframes timeLineLento
	0%
		opacity 0
	90%
		opacity 0
	100%
		opacity 1
