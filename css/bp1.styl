.footer	
	background rgba(azul,.8)
	color white
	height 100px
	overflow hidden
	padding 20px 
	position relative
	z-index 5
	.blanco
		position absolute
		left -15px
		bottom 10px
	.right
		float right
		margin 8px 0 0 0
		vertical-align bottom
		a
			color white
		span
			display inline-block
			font-weight 400
			&:after
				content " |"
		.email
			display inline-block
			&:after
				content email
				font-family icomoon
				vertical-align bottom
.listaDias
