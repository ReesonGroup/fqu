						.imagen
							position relative
							.imagenInner
								position absolute
								overflow hidden
								width 100%
								height 250px
								img
									width 100%
									height auto
									overflow hidden

								
						.sombra
							bottom 0
							box-shadow 0 -100px 100px rgba(black,.5) inset
							left 0
							position absolute
							right 0
							top 0
							z-index 1
				.plegado
