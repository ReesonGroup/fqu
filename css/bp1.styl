footer	
	background rgba(verde,.8)
	color texto
	overflow hidden
	padding 20px
	img
		float left
		height 50px
		opacity .8
		vertical-align middle
		width auto
	.right
		float right
		margin 8px 0 0 0
		vertical-align bottom
		span
			display inline-block
			font-weight 400
			&:after
				content " |"
		.email
			display inline-block
			&:after
				content email
				font-family icomoon
				vertical-align bottom
.header
	height 500px
	overflow hidden
	position relative
	width 100%
	z-index 5
	.heroOuter
		height 100%
		width 100%
		.hero
			background gris
			height 100%
			width 100%
			animation animacion 150s infinite
			-webkit-animation animacion 150s infinite
			.heroInner
				margin -50px 0 0 0 
				height 250%
				width 250%
				background-image url(../img/hero.jpg)
				background-position 0
				background-repeat repeat-x repeat-y 
				background-size 45% auto
				transform: skew(0deg, 10deg);
				-ms-transform: skew(0deg, 10deg);
				-webkit-transform: skew(0deg, 10deg);
				transform: scale(1, 2, 3, 4);
				-ms-transform: scale(1, 2, 3, 4);
				-webkit-transform: scale(1, 2, 3, 4);
	.logoOuter
		position absolute
		top 0 
		right 0
		bottom 0
		left 0
		@css {
			background: -moz-linear-gradient(left,  rgba(18,19,20,1) 0%, rgba(18,19,20,1) 39%, rgba(18,19,20,0) 100%);
			background: -webkit-gradient(linear, left top, right top, color-stop(0%,rgba(18,19,20,1)), color-stop(39%,rgba(18,19,20,1)), color-stop(100%,rgba(18,19,20,0)));
			background: -webkit-linear-gradient(left,  rgba(18,19,20,1) 0%,rgba(18,19,20,1) 39%,rgba(18,19,20,0) 100%);
			background: -o-linear-gradient(left,  rgba(18,19,20,1) 0%,rgba(18,19,20,1) 39%,rgba(18,19,20,0) 100%);
			background: -ms-linear-gradient(left,  rgba(18,19,20,1) 0%,rgba(18,19,20,1) 39%,rgba(18,19,20,0) 100%);
			background: linear-gradient(to right,  rgba(18,19,20,1) 0%,rgba(18,19,20,1) 39%,rgba(18,19,20,0) 100%);
			filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#121314', endColorstr='#00121314',GradientType=1 );

		}
		h1
			color texto
			font-size 24px
			font-weight 400
			letter-spacing 1px
			line-height 1.3em
			text-align center
			text-transform uppercase
			span
				font-weight 700
		.logoInner
			padding 45px 0 30px 0 
			figure
				img
					display block
					margin 0 auto
					width 220px
.llamadaLabel
	background verde
	float right
	transition all .5s
	-webkit-transition all .5s
	overflow hidden
	position relative
	z-index 6
	.boton
		color texto
		height 80px
		text-align center
		width 40px
		transition all .5s
		-webkit-transition all .5s
		transform rotateZ(0deg)
		-ms-transform rotateZ(0deg)
		-webkit-transform rotateZ(0deg)
		.flecha
			font-size 1.5em
			&:after
				content flecha
				font-family icomoon
				line-height 80px
.llamadaTitulo
	background negroInterface3
	font-weight 400
	height 80px
	width 100%
	transition all .5s
	-webkit-transition all .5s
	position relative
	z-index 5
	h2
		color texto
		font-size 21px
		font-style italic
		letter-spacing 1px
		padding 15px 0 0 30px
		text-transform uppercase
		span
			display block
			font-size 16px 
			letter-spacing 0px
			text-transform lowercase
.noCalendario
	position relative
	z-index 5
	.informacion
		background negroInterface3
		position relative
		.infoHeader
			color texto
			height 80px
			h2
				font-size 21px
				font-style italic
				letter-spacing 1px
				padding 15px 0 0 30px
				text-transform uppercase
				span
					display block
					font-size 16px 
					letter-spacing 0px
					line-height 21px
					text-transform lowercase
		.infoOuter
			.infoInner
				border-bottom 1px solid negroInterface3
				.checkInfo
					width 100%
					height 70px 
					background transparent
					position relative
					z-index 20
					.puntos
						&:after
							color texto
							content menu2
							font-family icomoon
							position absolute
							font-style normal
							top 20px
							right 20px
							padding 5px 0 5px 20px
				.entrades
					margin -70px 0 0 0
					&:before
						content entrada
						font-family icomoon
						font-style normal
						font-size 18px 
						left 20px
						position absolute
						top 30px
				.inscripcions
					margin -70px 0 0 0
					&:before
						content calendario
						font-family icomoon
						font-style normal
						font-size 18px 
						left 20px
						position absolute
						top 30px
				.horario
					margin -70px 0 0 0
					&:before
						content reloj
						font-family icomoon
						font-style normal
						font-size 18px 
						left 20px
						position absolute
						top 30px
				.notes
					margin -70px 0 0 0
					&:before
						content nota
						font-family icomoon
						font-style normal
						font-size 18px 
						left 20px
						position absolute
						top 30px
				.post
					background negroInterface
					color rgba(white,.7)
					font-size 16px
					font-style italic
					line-height 21px
					padding 28px 0 28px 0
					position relative
					h4
						margin 0 78px 0 55px
					.detallePost
						border-left none
						h5
							color rgba(white,1)
							font-style normal
							margin 20px 20px 10px 55px
							&:first-child
								margin 40px 20px 10px 55px
						p
							color rgba(white,.6)
							font-style normal
							font-weight 400
							margin 0 20px 0 55px
							&:last-child 
								margin 0 20px 30px 55px
						p.detallePostPiscina
							margin 20px 20px 30px 55px
						ul
							margin 20px 20px 0 55px
							li 
								color rgba(white,.6)
								font-style normal
								font-weight 400
								list-style-type circle
								margin 0 0 10px 0
								&:last-child 
									margin 0 0 30px 0
					&:last-child
						border-bottom none
	.colaboradors
		background white
		color rgba(black,.7)
		h3
			font-size 24px
			font-weight 300
			padding 20px
			text-align center
		.logoOuter
			// Este hace que los logos se coloquen mas o menos por fila
			padding 0 20px 200px 20px 
			// Este hace que los logos se coloquen mas o menos por fila
			text-align center
			.logoInner
				display inline-block
				padding 15px
				vertical-align top
				position relative
				// Este hace caber mas o menos logos
				width 60%
				// Este hace caber mas o menos logos
				a
					color rgba(black,.2)
				img
					width 100%
				.add
					height 100px
					position absolute
					left 30px

					width 100px
					p
						font-size 16px
						font-weight 400
						margin-top -10px
						text-align center
						width 150px
					&:before
						color rgba(black,.1)
						content add
						font-family icomoon
						font-size 150px
	.pubillas
		background white
		color rgba(black,.7)
		h3
			font-size 24px
			font-weight 300
			padding 20px
			text-align center
		.pubillaOuter
			display block
			font-size 0
			margin 0 auto
			padding 0 20px 20px 20px 
			text-align center
			.pubillaInner 
				// background red
				display inline-block
				margin 10px 0
				position relative
				width 150px
				img
					border-radius 50%
					box-shadow 1px 1px 3px rgba(black,.3)
					display block
					margin 0 auto
					width 100px
					.detall
						// background blue
						font-size 14px
						font-weight 300
						margin 12px 0 20px 0 
						padding 0 5px
						text-align center
						.nom
							font-weight 400

// Acciones CHECKBOX
#llamadaCalendario
	display none
#llamadaCalendario:checked ~ .llamadaLabel .boton 
	transform rotateZ(180deg)
	-ms-transform rotateZ(180deg)
	-webkit-transform rotateZ(180deg)