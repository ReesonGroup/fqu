// Acciones CHECKBOX
#llamadaCalendario
	display none
#llamadaCalendario:checked ~ footer
	width 100%
#llamadaCalendario:checked ~ .calendario 
	margin -80px 0 0 0 
#llamadaCalendario:checked ~ .contenidos 
	margin-left -100%
#llamadaCalendario:checked ~ .contenidos .calendario .listaDias 
	display block
#llamadaCalendario:checked ~ .decoracion .lineaOuter
	display block
#llamadaCalendario:checked ~ .decoracion .tituloOuter 
	display block

#llamadaCalendario:checked ~ .llamadaLabel
	background none
#llamadaCalendario:checked ~ .llamadaLabel .boton 
	transform rotateZ(180deg)
	-ms-transform rotateZ(180deg)
	-webkit-transform rotateZ(180deg)
#llamadaCalendario:checked ~ .llamadaTitulo
	margin-left -100%


// ANIMACIONES
@keyframes animacion
	0%
		transform translate(-150%, -150%)
		-ms-transform translate(-150%, -150%)
		-webkit-transform translate(-150%, -150%)
	50%
		transform translate(40%, 10%)
		-ms-transform translate(40%, 10%)
		-webkit-transform translate(40%, 10%)

	100%
		transform translate(-150%, -150%)
		-ms-transform translate(-150%, -150%)
		-webkit-transform translate(-150%, -150%)

@keyframes timeLine
	0%
		opacity 0
	100%
		opacity 1

@keyframes timeLineLento
	0%
		opacity 0
	90%
		opacity 0
	100%
		opacity 1


	0%
		opacity 0
	90%
